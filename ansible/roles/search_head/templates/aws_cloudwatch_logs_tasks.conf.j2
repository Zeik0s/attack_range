[aws]
account = splunk_role_{{ key_name }}
delay = 1800
groups = API-Gateway-Execution-Logs_{{ api_gateway_id }}/prod, /aws/lambda/notes_application_{{ key_name }}, RDSOSMetrics:log-stream:{{ db_id }}
index = aws
interval = 60
only_after = 1970-01-01T00:00:00
region = {{ region }}
sourcetype = aws:cloudwatchlogs
stream_matcher = .*

[aws_db]
account = splunk_role_{{ key_name }}
delay = 1800
groups = RDSOSMetrics
index = aws
interval = 60
only_after = 1970-01-01T00:00:00
region = {{ region }}
sourcetype = aws:cloudwatchlogs
stream_matcher = {{ db_id }}
